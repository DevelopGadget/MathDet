<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e0e0e0"
    tools:context="mathdet.mathgadget.com.mathdet.tres">
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A = "
        android:textColor="@android:color/background_dark"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="10dp"
        android:layout_height="295dp"
        app:srcCompat="@drawable/linea"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_editor_absoluteY="78dp"
        tools:layout_editor_absoluteX="70dp"
        app:layout_constraintVertical_bias="0.497" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="10dp"
        android:layout_height="304dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/linea"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.889"
        app:layout_constraintVertical_bias="0.497" />

    <EditText
        android:id="@+id/X5"
        style="@style/Base.TextAppearance.AppCompat.Display2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusDown="@+id/X3"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="342dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="120dp" />

    <Button
        android:id="@+id/btnResolver"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="41dp"
        android:background="@drawable/pencil"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="90dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.077" />

    <Button
        android:id="@+id/btnLimpiar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="41dp"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="90dp"
        android:background="@drawable/garbage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/btnResolver"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/X7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:ems="10"
        android:textColor="@android:color/background_dark"
        android:imeOptions="actionNext"
        android:background="@drawable/edit"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="342dp"
        android:layout_marginRight="10dp"
        app:layout_constraintRight_toRightOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="120dp"
        app:layout_constraintVertical_bias="0.509" />

    <EditText
        android:id="@+id/X6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:textColor="@android:color/background_dark"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="342dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="120dp"
        app:layout_constraintVertical_bias="0.509"
        app:layout_constraintLeft_toRightOf="@+id/X5"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toLeftOf="@+id/X7"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.562" />

    <EditText
        android:id="@+id/X13"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:ems="10"
        android:imeOptions="actionGo"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:background="@drawable/edit"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="142dp"
        android:layout_marginRight="10dp"
        app:layout_constraintRight_toRightOf="@+id/imageView4" />

    <EditText
        android:id="@+id/X12"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:nextFocusForward="@+id/X13"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="140dp"
        app:layout_constraintLeft_toRightOf="@+id/X11"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toLeftOf="@+id/X13"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.575" />

    <EditText
        android:id="@+id/X11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:nextFocusForward="@+id/X12"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="143dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView3" />

    <EditText
        android:id="@+id/X10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:imeOptions="actionNext"
        android:background="@drawable/edit"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_dark"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/X7"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/X13"
        app:layout_constraintVertical_bias="0.549"
        android:layout_marginRight="10dp"
        app:layout_constraintRight_toRightOf="@+id/imageView4" />

    <EditText
        android:id="@+id/X9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edit"
        android:textColor="@android:color/background_dark"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/X12"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintLeft_toRightOf="@+id/X8"
        app:layout_constraintRight_toLeftOf="@+id/X10"
        app:layout_constraintTop_toBottomOf="@+id/X6"
        app:layout_constraintVertical_bias="0.548" />

    <EditText
        android:id="@+id/X8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:textColor="@android:color/background_dark"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/X5"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/X11"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.536" />

</android.support.constraint.ConstraintLayout>
