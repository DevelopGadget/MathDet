<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e0e0e0"
    tools:context="mathdet.mathgadget.com.mathdet.tres">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="A = "
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Texto_dos"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="10dp"
        android:layout_height="@dimen/Linea_tres"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toTopOf="@+id/btnResolver"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        app:srcCompat="@drawable/linea" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="10dp"
        android:layout_height="@dimen/Linea_tres"
        android:layout_marginBottom="50dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/btnLimpiar"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/linea" />

    <EditText
        android:id="@+id/X5"
        style="@style/Base.TextAppearance.AppCompat.Display2"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusRight="@+id/X6"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <Button
        android:id="@+id/btnResolver"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="41dp"
        android:layout_marginStart="8dp"
        android:background="@mipmap/lapiz"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3" />

    <Button
        android:id="@+id/btnLimpiar"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="41dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/borrador"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView4" />

    <EditText
        android:id="@+id/X7"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusDown="@id/X8"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/X10"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <EditText
        android:id="@+id/X6"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusRight="@id/X7"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.562"
        app:layout_constraintLeft_toRightOf="@+id/X5"
        app:layout_constraintRight_toLeftOf="@+id/X7"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <EditText
        android:id="@+id/X13"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionDone"
        android:inputType="number|numberSigned|numberDecimal"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toStartOf="@+id/imageView4" />

    <EditText
        android:id="@+id/X12"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusForward="@+id/X13"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintLeft_toRightOf="@+id/X11"
        app:layout_constraintRight_toLeftOf="@+id/X13" />

    <EditText
        android:id="@+id/X11"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusForward="@+id/X12"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintStart_toEndOf="@+id/imageView3" />

    <EditText
        android:id="@+id/X10"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusDown="@id/X11"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/X13"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/X7" />

    <EditText
        android:id="@+id/X9"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusForward="@id/X10"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/X12"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintLeft_toRightOf="@+id/X8"
        app:layout_constraintRight_toLeftOf="@+id/X10"
        app:layout_constraintTop_toBottomOf="@+id/X6" />

    <EditText
        android:id="@+id/X8"
        android:layout_width="@dimen/Cuadro_tres"
        android:layout_height="@dimen/Cuadro_tres"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/edit"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="number|numberSigned|numberDecimal"
        android:nextFocusForward="@id/X9"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/Edit_dos"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/X11"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/X5" />

</android.support.constraint.ConstraintLayout>
